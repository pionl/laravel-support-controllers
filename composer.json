{
  "name": "pion/laravel-support-controllers",
  "description": "A set of support classes/traits for Laravel Controller",
  "require": {
    "illuminate/support": "5.*"
  },
  "suggest": {
    "atorscho/crumbs": "Required to use with CrumbsNavigationTrait (~2.1.5) and laravel framework (5.1.*)."
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Pion\\Support\\Controllers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\" : "tests/"
    },
    "classmap": [
      "tests/Mock/URL.php"
    ]
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Kluska",
      "email": "martin.kluska@imakers.cz"
    }
  ],
  "minimum-stability": "stable"
}
